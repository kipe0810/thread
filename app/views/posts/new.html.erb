<div class="container">
	<div class="row">
		<div class="form-area col-xs-6 col-xs-offset-3">
			<h1 class="post-new-headline">新規投稿</h1>
			<%= form_for(@post) do |f| %>
				<table class="table">
					<tr>
						<th>タイトル</th>
						<td><%= f.text_field :title, class: "form-control", style:"width: 70%;" %></td>
					</tr>
				</table>
				<%= f.submit class: "btn btn-default" %>
			<% end %>
		</div>
	</div>
</div>


